	<queryString>
		<![CDATA[SELECT
facturas.id,
facturas.identificacion,
facturas.omitir_receptor,
facturas.receptor_nombre,
facturas.receptor_tipo_identif,
facturas.receptor_num_identif,
(
CONCAT((SELECT 
CONCAT(c_provincias.provincia,', ',c_cantones.canton,', ',c_distritos.distrito,', ',IFNULL(c_barrios.barrio,''))
FROM
c_provincias
INNER JOIN c_cantones ON c_cantones.provincia_id = c_provincias.id
INNER JOIN c_distritos ON c_distritos.canton_id = c_cantones.id
LEFT JOIN c_barrios ON c_barrios.distrito_id = c_distritos.id
WHERE c_provincias.id = facturas.receptor_provincia AND
c_cantones.numero_canton = facturas.receptor_canton AND c_distritos.numero_distrito = facturas.receptor_distrito AND
c_barrios.numero_barrio = facturas.receptor_barrio),' , ',facturas.receptor_otras_senas)
) AS direccion_receptor,
facturas.situacion,
facturas.terminal,
facturas.tipo_documento,
IFNULL(facturas.cod_moneda,'CRC') cod_moneda,
facturas.cond_venta,
facturas.medio_pago,
facturas.otros,
facturas.plazo_credito,
facturas.receptor_cod_pais_fax,
facturas.receptor_cod_pais_tel,
facturas.receptor_email,
facturas.receptor_fax AS receptor_fax_a,
facturas.receptor_tel AS receptor_tel_a,
CONCAT('+(',facturas.receptor_cod_pais_tel,') ',facturas.receptor_fax) AS receptor_fax_b,
CONCAT('+(',facturas.receptor_cod_pais_tel,') ',facturas.receptor_tel) AS receptor_tel_b,
facturas.tipo_cambio,
facturas.total_comprobante,
facturas.total_descuentos,
facturas.total_venta_neta,
facturas.total_exentos,
facturas.total_gravados,
facturas.total_imp,
facturas.total_merc_exentas,
facturas.total_merc_gravadas,
facturas.total_serv_exentos,
facturas.total_serv_gravados,
facturas.total_ventas,
facturas.clave,
facturas.consecutivo,
facturas.emisor_fax AS emisor_fax_a,
facturas.emisor_tel AS emisor_tel_a,
CONCAT('+(',facturas.emisor_cod_pais_fax,') ',facturas.emisor_fax) AS emisor_fax_b,
CONCAT('+(',facturas.emisor_cod_pais_tel,') ',facturas.emisor_tel) AS emisor_tel_b,
facturas.emisor_email,
facturas.emisor_nombre,
facturas.emisor_num_identif,
facturas.emisor_tipo_identif,
DATE_FORMAT(facturas.fecha_emision,'%d/%m/%Y - %r') fecha_emision,
facturas.nombre_comercial,
facturas.numero_factura,
facturas.medio_pago_2,
facturas.medio_pago_3,
facturas.medio_pago_4,
IFNULL(facturas.total_exonerado,0.00) total_exonerado,
IFNULL(facturas.total_iva_devuelto,0.00) total_iva_devuelto,
IFNULL(facturas.total_merc_exonerada,0.00) total_merc_exonerada,
IFNULL(facturas.total_otros_cargos,0.00) total_otros_cargos,
IFNULL(facturas.total_serv_exonerado,0.00) total_serv_exonerado,
facturas.codigo_actividad,

CONCAT(c_provincias.provincia,', ',
		c_cantones.canton,
		', ',
		c_distritos.distrito,
		', ',
		IFNULL(c_barrios.barrio,''),
		', ',
		emisores.otras_senas 
	) AS direccion_emisor,

emisores.detalle_en_factura1,
emisores.detalle_en_factura2,
c_condicion_de_venta.condicion_venta,
c_medios_de_pago.medio_de_pago
FROM
facturas
LEFT JOIN c_provincias ON c_provincias.id = facturas.emisor_prov
LEFT JOIN c_cantones ON c_cantones.numero_canton = facturas.emisor_canton AND c_cantones.provincia_id = c_provincias.id
LEFT JOIN c_distritos ON c_distritos.numero_distrito = facturas.emisor_distrito AND c_distritos.canton_id = c_cantones.id
LEFT JOIN c_barrios ON c_barrios.numero_barrio = facturas.emisor_barrio AND c_barrios.distrito_id = c_distritos.id
LEFT JOIN emisores ON emisores.identificacion = facturas.identificacion
LEFT JOIN c_condicion_de_venta ON c_condicion_de_venta.id = facturas.cond_venta
LEFT JOIN c_medios_de_pago ON c_medios_de_pago.id = facturas.medio_pago

WHERE
facturas.clave =  $P{CLAVE_FACTURA}]]>
	</queryString>
